@model IList<facebook_asp_.Models.friends>


<style>
    .user {
        border-style: solid;
        border-color: rgb(45, 52, 129);
        margin: 20px;
        padding: 10px;
    }
</style>


<div class="row">
    @{foreach (var item in Model)
        {
            <div class="col-sm-4 col-xs-12 user">
                <img src="@item.userFriend.photo" height="100px" />
                <h4>@item.userFriend.Fname @item.userFriend.lname</h4>
                <a href="/Porfile/Myfriend/@item.userFriend.Id">Go To Porfile</a>
            </div>
        }
    }
</div>